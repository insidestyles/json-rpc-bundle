{
    "name": "insidestyles/json-rpc-bundle",
    "description": "A simple and fast way to create json rpc server api as microservice. Json Rpc 2.0 Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "fuong",
            "email": "insidestyles@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/framework-bundle": "^4.1",
        "zendframework/zend-json-server": "^3.0",
        "doctrine/annotations": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "jms/serializer-bundle": "^3.0",
        "symfony/messenger": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/serializer": "^4.1",
        "ocramius/proxy-manager": "^2.2"
    },
    "suggest": {
        "doctrine/annotations": "Install for annotation features",
        "jms/serializer-bundle": "Allows JMS Serializer",
        "symfony/messenger": "Use Symfony Messenger",
        "symfony/serializer": "Allows Symfony Serializer",
        "symfony/validator": "Use Symfony Validator for message bus validation",
        "ocramius/proxy-manager": "Use ocramius/proxy-manager for remote service call"
    },
    "autoload": {
        "psr-4": {
            "Insidestyles\\JsonRpcBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "Tests/DependencyInjection"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
