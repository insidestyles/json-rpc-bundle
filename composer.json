{
    "name": "insidestyles/json-rpc-bundle",
    "description": "Json Rpc Bundle For MSA",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "fuong",
            "email": "insidestyles@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/framework-bundle": "^3.4|^4.1",
        "zendframework/zend-json-server": "^3.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.0",
        "phpunit/phpunit": "^6.5",
        "jms/serializer-bundle": "^3.0",
        "symfony/messenger": "^4.1",
        "symfony/validator": "^3.4|^4.1",
        "symfony/serializer": "^3.4|^4.1"
    },
    "suggest": {
        "doctrine/annotations": "Install for annotation features",
        "jms/serializer-bundle": "Allows JMS Serializer",
        "symfony/messenger": "Use Symfony Messenger",
        "symfony/serializer": "Allows Symfony Serializer",
        "symfony/validator": "Use Symfony Validator for message bus validation"
    },
    "autoload": {
        "psr-4": {
            "Insidestyles\\JsonRpcBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "Tests/DependencyInjection"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
